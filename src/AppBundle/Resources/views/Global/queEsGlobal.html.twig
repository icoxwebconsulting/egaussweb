{% extends "::frontbase_globalimast.html.twig" %}
{%block body%}



    <!----------------------------------------------------Centre-------------------------------------------------->
    <div class="wrapperCenter">
        <div class="mainContentWrapper">
            <div class="row">
                {% include 'AppBundle:Global:menuLeftGlobalImasT.html.twig' with {"active":"que_es_global"} %}
                <div class="col s7 m12 l6 left">
                    <h6 class="titlemenu" style="text-align: left !important; padding: 0.7em !important;">{{ entity.titulo }}</h6>
                    <div class="col s12 m12 l12 left">
                        <p style="text-align: justify;" >{{ entity.texto }}</p>
                    </div>
                </div>
                <div class="col s3 m12 l3">
                    <h2 class="content-txt-img"></h2>
                    <div class="video-container top10">
                        {{ entity.urlvideo | raw }}
                    </div>
                    {{ render(controller('AppBundle:Default:videos', { 'owner': "que-es-global" })) }}
                    <div class="row top10">
                        {{ render(controller('AppBundle:Default:archivos', { 'owner': "que-es-global" })) }}
                    </div>
                </div>
            </div>







        </div>
        </div>
{%endblock body%}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/app/js/menu.js') }}" type="text/javascript"></script>
{% endblock %}