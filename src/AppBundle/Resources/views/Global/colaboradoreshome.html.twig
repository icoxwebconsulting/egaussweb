{% for entity in colaboradores %}
    <div class="row">
        <div class="col s3 m3 l4">
            <a href=" {{ path("colaborador", {"slug": entity.slug}) }} "><img class="responsive-img" src="{{ asset(entity.getThumbnail()) }}"/></a>
        </div>
        <div class="col s5 m5 l8">
            <a class="justify noticetitleindex" href=" {{ path("colaborador", {"slug": entity.slug}) }} "> {{ entity.name | truncate(100) }}</a>
        </div>
    </div>
{% endfor %}