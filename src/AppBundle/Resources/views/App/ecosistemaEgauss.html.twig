{%extends "::frontbase.html.twig"%}
{%block body%}


    <!----------------------------------------------------Centre-------------------------------------------------->
    <div class="wrapperCenter">
        <div class="mainContentWrapper">
            <div class="row">
            <div class="col s7 m12 l9">
                <h6 class="titlemenu" style="text-align: left !important; padding: 0.7em !important;">{{ entity.titulo }}</h6>

                    <div class="col s12 m12 l12 left">
                        <div class="col s5">
                            <img class="responsive-img activator card-profile-image materialboxed" alt="{{ entity.titulo }}" src="{{ asset(entity.webPath) }}">

                        </div>
                        <p class="justify">{{ entity.texto | raw }}</p>
                    </div>
        </div>
            <div class="col s3 m12 l3">
                <h2 class="content-txt-img"></h2>
                <div class="video-container top10">
                    {{ entity.urlvideo | raw }}
                </div>

            </div>
        </div>
        </div>
        </div>


{%endblock body%}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/app/js/menu.js') }}" type="text/javascript"></script>
{% endblock %}