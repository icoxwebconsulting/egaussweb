    <!--breadcrumbs start-->
    <div id="breadcrumbs-wrapper" class="grey lighten-3">

        <div class="container">
            <div class="row">
                <div class="col s12 m12 l12">
                    <h5 class="breadcrumbs-title">Colaboradores</h5>
                    <ol class="breadcrumb">
                        <li><a href="{{ path("dashboard") }}">Panel de Administraci&oacute;n</a>
                        </li>
                        <li><a href="#">Ver Colaborador</a>
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
    <!--breadcrumbs end-->


    <!--start container-->
    <div class="container">
         <div class="row">
            <div class="col s12">
                <ul class="tabs">
                    <li class="tab col s12"><a class="active" href="#datos">Datos del Colaborador</a></li>
                    <li class="tab col s12"><a href="#evento">Eventos del Colaborador</a></li>
                    <li class="tab col s12"><a href="#video_colaborador_tab">Videos del Colaborador</a></li>
                </ul>
            </div>
            <div id="datos" class="col s12">
                <div class="col s3">
                <img class="circle responsive-img activator card-profile-image" alt="" src="{{ asset(entity.webPath) }}">

                </div>
                <h5 class="card-title activator grey-text text-darken-4">{{ entity.name }}</h5>
                <p><i class="cyan-text text-darken-2"></i> {{ entity.descripcion }}</p>
            </div>
            <div id="evento" class="col s12">
                {{ render(controller('BackendBundle:Backend:evento', { 'owner': entity.id })) }}
            </div>
            <div id="video_colaborador_tab" class="col s12">
                {{ render(controller('BackendBundle:Backend:video_colaborador', { 'owner': entity.id })) }}
            </div>
        </div>
        <div class="divider"></div>
    </div>

    <script>
        $('ul.tabs').tabs();
        $(".action").on("click", function(e){
            $('.preloader-wrapper').addClass('active');
            var url = $(this).attr("url");
            $("#main-wrapper").load(url,function() {
                $('.preloader-wrapper').removeClass('active');
            });

        })

    </script>
