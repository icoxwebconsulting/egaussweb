 <div class="container">

     <div class="col s12 m12 l6">
         <div class="card-panel">
             <h4 class="header2">Ingrese los datos para el registro.</h4>
             <div class="row">
                 <form class="col s12" id="form"  action="{{path('_global2016_create')}}" method="post" enctype="multipart/form-data">
                     {{ form_widget(form) }}
                     <div class="input-field col s12">
                         <button name="action" type="submit" class="btn cyan waves-effect waves-light center load">Guardar
                             <i class="mdi-content-save right"></i>
                         </button>
                     </div>

                 </form>
             </div>
         </div>
     </div>
     </div>

     <script>
         var options = {
             beforeSubmit:  showRequest,
             success:       showResponse
         };

         function showRequest(){
             $('.preloader-wrapper').addClass('active');
         }
         function showResponse(responseText, statusText, xhr, $form)  {
             $('.preloader-wrapper').removeClass('active');
             var obj = jQuery.parseJSON(responseText);
             if(obj.status){
                 var url = "{{ path("_global2016") }}";
                 $("#main-wrapper").load(url);
                 Materialize.toast(obj.message, 1000);
             }
             else{
                 Materialize.toast(obj.message, 1000);
             }

         }
         $('#form').ajaxForm(options);
     </script>
    
