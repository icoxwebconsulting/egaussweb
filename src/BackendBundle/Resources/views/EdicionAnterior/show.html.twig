
     <div id="breadcrumbs-wrapper" class="grey lighten-3">

         <div class="container">
             <div class="row">
                 <div class="col s12 m12 l12">
                     <h5 class="breadcrumbs-title">Imagenes</h5>
                     <ol class="breadcrumb">
                         <li><a href="{{ path("dashboard") }}">Panel de Administraci&oacute;n</a>
                         </li>
                         <li><a href="#">Ediciones Anteriores</a>
                         </li>
                         <li class="active">Imagenes</li>
                     </ol>
                 </div>
             </div>
         </div>
     </div>

     <div class="container">
         {{ render(controller('BackendBundle:Backend:imagenes', { 'owner': entity.generaOwner, 'entityid': entity.id })) }}
     </div>
     <script>
         $(".action").on("click", function(e){
             $('.tooltipped').tooltip('remove');
             $('.preloader-wrapper').addClass('active');
             var url = $(this).attr("url");
             $("#main-wrapper").load(url,function() {
                 $('.preloader-wrapper').removeClass('active');
             });

         })




     </script>
